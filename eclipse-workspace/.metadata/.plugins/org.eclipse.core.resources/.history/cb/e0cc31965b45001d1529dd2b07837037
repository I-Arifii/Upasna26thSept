package unit;

public class Calculator {
	
	public static int findBig(int x,int y) {
		
	public static int findSmall(int x, int y) {
		
	public static int findBiggest(int x, int z) {
		
	public int findMax(int[] arr) {
		int max = 0;
		for (int i = 0; i < arr.length; i++) {
			if (max < arr[i]) {
				max = arr[i];
			}
		    return max;
	}